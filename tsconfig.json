{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@components/*": ["src/components/*"],
      "@screens/*": ["src/components/screens/*"],
      "@services/*": ["src/services/*"],
      "@context/*": ["src/context/*"],
      "@models/*": ["src/components/types/*"],
      "@constants/*": ["constants/*"]
    }
  },
  "files.exclude": {
    "node_modules": true
  },
  "typescript.validate.enable": false
}
